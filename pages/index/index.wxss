/**index.wxss**/
.search-form{width: 100%}
.search-box{
  width: 100%;
  background: linear-gradient(to right,#ff827b,#ff657b); 
}
.input{width: 90%;padding: 5px 0;margin: 0 auto;background: #fff0f2;border-radius: 50px;text-align: center;border: 1px solid #FFF;color: #434343}
.placeholder{background: url('data:image/jpeg;base64,') no-repeat 100px center}

.btn-area button{background: url('data:image/jpeg;base64,') no-repeat center;width: 150px;height:38px;border: none;background-size: 100% 100%;font-size: 0;text-indent: -9999px;overflow: hidden}

.btn-area button::before,.btn-area button::after{display: none}
.btn-area{margin-top: 15px}
.app-tit{font-size: 16px;color: #333;border-left:3px solid #fd6265;text-indent: 5px;font-weight: 600}
.items{margin-top: 15px;border-radius: 10px}
.item{width: 30%;padding: 10px 0;text-align: center;box-shadow: 0 0 10px rgba(255, 118, 133, .5);margin-bottom: 15px}
.item navigator{width: 100%;font-size: 14px;color:  #434343;}
.items .fm{left: 5%;position: relative}
.item image{width: 45px;height: 45px;}